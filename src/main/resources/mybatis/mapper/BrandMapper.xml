<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.flab.fkream.brand.BrandMapper">
    <insert id="insertBrand" parameterType="com.flab.fkream.brand.Brand">
        INSERT INTO BRAND(id, brand_name, luxury)
        VALUES (#{id}, #{brandName}, #{luxury})
    </insert>

</mapper>